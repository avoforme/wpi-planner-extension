{
    "manifest_version": 3,
    "name": "Chrome Extension for WPI Planner",
    "version": "1.0.0",
    "description": "A simple React app as a Chrome extension",
    "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["./ratemydata/*"],
        "matches": ["https://planner.wpi.edu/*"]
      }
    ],
    "host_permissions": [
      "https://planner.wpi.edu/"
    ],
     "content_scripts": [
      {
        "matches": ["https://planner.wpi.edu/*"],
        "js": ["append.js", "findName.js"]
      }
     ],
     "background": {
      "service_worker": "worker.js",
      "type": "module"
    },
    "icons": {
      "128": "logo.png"
    }
  }