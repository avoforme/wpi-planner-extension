{
    "manifest_version": 3,
    "name": "Rate My Planner",
    "version": "0.1.4",
    "description": "A simple chrome extension that shows professors Rate My Professor rating in WPI Planner",
    "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["./ratemydata/*"],
        "matches": ["https://planner.wpi.edu/*"]
      }
    ],
     "content_scripts": [
      {
        "matches": ["https://planner.wpi.edu/*"],
        "js": ["append.js", "findName.js"]
      }
     ],
     "background": {
      "service_worker": "worker.js",
      "type": "module"
    },
    "icons": {
      "128": "logo.png"
    }
  }